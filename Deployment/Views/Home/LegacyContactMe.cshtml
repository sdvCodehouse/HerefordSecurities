
@{
    Layout = null;
}

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Hereford Securities Ltd</title>
    <meta name="description" content="This site provides information about the services and client history of Hereford Securities Ltd. Trading company for Simon Da Vall" />
    <meta name="keywords" content="Simon Da Vall, Da Vall, Hereford Securities, HSL, C#, C# Development, Web Development, Asp.Net" /> 

    <link rel="Stylesheet" href="~/css/reset.css" />
    <link rel="Stylesheet" href="~/css/hsl.css" />

    <script src="~/scripts/hsl.js" type="text/javascript"></script>
</head>

<body>
<div id="container">
    <div id="header">
        <div id="logo">
            <img id="HSL_logo" src="~/images/HSL_Logo.gif" alt="Hereford Securities Ltd" />
        </div>
        <div id="navbar">
            @Html.ActionLink("HOME", "Index") | @Html.ActionLink("SERVICES", "Services") | @Html.ActionLink("CONTACT", "ContactMe") <br/>
            @Html.ActionLink("LEGACY HOME", "LegacyHome") | @Html.ActionLink("LEGACY SERVICES", "LegacyServices") | @Html.ActionLink("LEGACY CONTACT", "LegacyContactMe")
        </div>
    </div>

    <!-- Put blue and grey boxes in this section -->


    <div id="mainContent">
        <div id="contentColumn">
            <h2>Contact Us</h2>
            <div class="contactDetails bluelinks">
                <p class="addressDetails">
                    <strong>Hereford Securities Limited</strong><br />
                    38 Gibbon Road<br />
                    Kingston-Upon_Thames<br />
                    Surey, KT2 6AB
                </p>
                <p class="phoneDetails">
                    T: +44 (0)7740 423523<br />
                    E: <script type="text/javascript">disea();</script></p>
            </div>
            <!-- end of contactDetails section -->
            <div id="map_canvas">
                <iframe frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="http://maps.google.co.uk/maps?f=q&amp;source=s_q&amp;hl=en&amp;geocode=&amp;q=38+Gibbon+Road,+Kingston-Upon-Thames,+Surrey,+KT2+6AB&amp;sll=53.800651,-4.064941&amp;sspn=13.387936,35.859375&amp;ie=UTF8&amp;s=AARTsJoVSMKyw-OBt_yqVLJsfK4aQXL7dQ&amp;ll=51.423243,-0.294657&amp;spn=0.018732,0.036478&amp;z=14&amp;iwloc=addr&amp;output=embed"></iframe><br /><small><a href="http://maps.google.co.uk/maps?f=q&amp;source=embed&amp;hl=en&amp;geocode=&amp;q=38+Gibbon+Road,+Kingston-Upon-Thames,+Surrey,+KT2+6AB&amp;sll=53.800651,-4.064941&amp;sspn=13.387936,35.859375&amp;ie=UTF8&amp;ll=51.423243,-0.294657&amp;spn=0.018732,0.036478&amp;z=14&amp;iwloc=addr" style="color:#0000FF;text-align:left">View Larger Map</a></small>   
            </div>
    
            <div class="registeredOffice">
                <strong>Registered Office:</strong><br />
                Linkline House<br />
                65 Church Road<br />
                Hove<br />
                East Sussex, BN3 2BD
            </div>
            <div class="registrationNumber">
                Registered in England. Number: 2100431<br />
            </div>
    
        </div>
        <!-- end of contentColumn section -->
        <div id="rightdevices">
            <div class="bluebox bluelinks">
                <h3>Useful Links</h3>
                <ul>
                    <li><a href="http://www.microsoft.com">Microsoft</a></li>
                    <li><a href="http://www.experts-exchange.com/">Experts Exchange</a></li>
                    <li><a href="http://headfirstlabs.com/">Head First Books</a></li>
                    <li><a href="http://www.bcs.org/">British Computer Society</a></li>
                </ul>
            </div>
            <img class="bluebox_base" alt="Bluebox bottom" src="~/images/bluebox_btmcap.gif" />
            <img class="greybox_topcap" src="~/images/greybox_topcap.gif" width="198" height="6" alt="" border="0" />

            <div class="greybox bluelinks">
                <p><strong>Other Links</strong></p>
                <p>
                    <a href="http://en.wikipedia.org/wiki/Address_munging">Email Address Munging</a><br />
                    <a href="http://code.google.com/apis/maps/">Google Maps Api</a><br />
                </p>
            </div>
            <img class="greybox_btmcap" src="~/images/greybox_btmcap.gif" width="198" height="6" alt="" border="0"  />    
        </div>
        <!-- end of rightDevices section -->
    </div>
    <!-- end of mainContent section -->  
    <div id="footer">
        <div id="footernav">
            <a href="index.htm">HOME</a> | <a href="services.htm">SERVICES</a> | <a href="clients.htm">
                CLIENTS</a> | <a href="cv.htm">CV</a> | <a href="contactus.htm">CONTACT US</a>
        </div>
        <p>&copy;2009 <a href="index.htm">Hereford Securities Ltd</a></p>
    </div>
    <!-- end of footer section -->
</div>
<!-- end of container section -->

</body>
</html>
